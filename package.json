{
  "name": "@riddance/aws-host",
  "version": "0.0.10",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "riddance",
    "serverless",
    "environment",
    "devops",
    "aws",
    "opinionated"
  ],
  "files": [
    "*.js",
    "lib/*.js"
  ],
  "exports": {
    "./context": "./context.js",
    "./http": "./http.js"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "5.2.0",
    "@smithy/signature-v4": "2.3.0",
    "@riddance/service": "0.1.0",
    "@riddance/host": "0.1.0",
    "@riddance/fetch": "0.1.0"
  },
  "devDependencies": {
    "@riddance/env": "0.5.0"
  },
  "scripts": {
    "start": "riddance-watch",
    "prepack": "riddance-build",
    "prepublishOnly": "riddance-prepublish",
    "postpublish": "riddance-sync"
  }
}
